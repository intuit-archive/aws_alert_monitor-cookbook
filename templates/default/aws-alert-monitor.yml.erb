<%= @name %>:
  access_key: <%= @access_key %>
  secret_key: <%= @secret_key %>
  region: <%= @region %>
  sqs_endpoint: <%= @sqs_endpoint %>
  events:
<% @events.each_pair do |event_name, details| %>
    <%= event_name %>:
      email:
        source: <%= details.source %>
        destination: <%= details.destination %>
<% end %>
